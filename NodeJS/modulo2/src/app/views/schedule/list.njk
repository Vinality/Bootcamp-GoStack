<label>
  <ul class="providers">
    {% for appointment in appointments %}
      <li class="provider">
        <div>
          <img src="/files/{{appointment.user.avatar}}"/>
          <strong>{{appointment.user.name}} as {{appointment.date | date("HH:mm")}}</strong>
        </div>
      </li>
    {% endfor %}
  </ul>
</label>
